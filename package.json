  "name": "logistics-app",
  "version": "1.0.0",
  "description": "A comprehensive logistics management platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
